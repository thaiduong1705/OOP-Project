@model List<Kinang>
@{
    ViewData["Title"] = "Home Page";
}

<partial name="_Introduction" />


<div id="portfolio">
    <table class="table table-bordered table-striped w-100">
        <thead>
            <tr>
                <th>STT</th>
                <th>Tên kĩ năng</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var p in Model)
            {
                <tr>
                    <td width="20%">@p.MaKiNang</td>
                    <td width="80%">@p.TenKiNang</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<!-- About -->
<partial name="_About" />

<!-- Contact -->
<partial name="_Contact" />
